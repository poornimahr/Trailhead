<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Revenue_Validation</fullName>
    <active>false</active>
    <errorConditionFormula>AnnualRevenue &gt;=  (20 * $CustomMetadata.Support_Tier__mdt.Gold.Minimum_Spending__c)</errorConditionFormula>
    <errorMessage>annual revenue must be at least 20 times the minimum spending amount of the Gold Support Tier.</errorMessage>
</ValidationRule>
